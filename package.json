{
  "name": "astra-bot",
  "version": "2.16.0",
  "description": "Astra - All-in-One Discord Bot with Web Dashboard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:bot": "tsx watch src/bot/index.ts",
    "dev:api": "tsx watch src/api/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"cd dashboard && npm run dev\"",
    "build": "npm run build:bot && npm run build:dashboard",
    "build:bot": "tsc",
    "build:dashboard": "cd dashboard && npm install && npm run build",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "NODE_ENV=development tsx src/index.ts",
    "postinstall": "cd dashboard && npm install",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:seed": "tsx src/database/seed.ts",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:migrate:status": "tsx scripts/migrate.ts status",
    "db:migrate:rollback": "tsx scripts/migrate.ts rollback",
    "commands:deploy": "tsx src/bot/deploy-commands.ts",
    "pelican:start": "npm run build && npm run start",
    "release": "tsx scripts/build-release.ts",
    "release:obfuscated": "npm run build && tsx scripts/build-release.ts --obfuscated-only",
    "release:source": "tsx scripts/build-release.ts --source-only"
  },
  "keywords": [
    "discord",
    "bot",
    "dashboard",
    "typescript",
    "astra"
  ],
  "author": "Astra Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@discord-player/extractor": "^7.1.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@napi-rs/canvas": "^0.1.83",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "chokidar": "^5.0.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cron": "^4.3.4",
    "discord-player": "^7.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "ffmpeg-static": "^5.3.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "pdfkit": "^0.17.2",
    "rate-limiter-flexible": "^9.0.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-discord": "^0.1.15",
    "@types/pdfkit": "^0.17.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "javascript-obfuscator": "^4.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
